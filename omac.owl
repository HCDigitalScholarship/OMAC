        <skos:prefLabel xml:lang="en">Observation</skos:prefLabel>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.omac.org#Observation"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#concerns"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://dbpedia.org/ontology/Person"/>
                            <rdf:Description rdf:about="http://dbpedia.org/ontology/Place"/>
                            <rdf:Description rdf:about="http://www.omac.org#Attribute"/>
                            <rdf:Description rdf:about="http://www.omac.org#Expression"/>
                            <rdf:Description rdf:about="http://www.omac.org#MusicalEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:annotatedTarget>
        <rdfs:comment>In principle observations can concern all domain entities</rdfs:comment>
    </owl:Axiom>
    


