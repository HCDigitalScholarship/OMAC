                            <rdf:Description rdf:about="http://dbpedia.org/ontology/Place"/>
                            <rdf:Description rdf:about="http://www.omac.org#Attribute"/>
                            <rdf:Description rdf:about="http://www.omac.org#Expression"/>
                            <rdf:Description rdf:about="http://www.omac.org#MusicalEntity"/>
                        </owl:unionOf>
