        <rdfs:subClassOf rdf:nodeID="genid80"/>
        <rdfs:comment>A claim is a scholarly observation (statement) of a musical phenomenon according to which the latter is characterised by a scholar in terms of various properties</rdfs:comment>
        <rdfs:comment xml:lang="en">The list of claims hereby included is not exhaustive</rdfs:comment>
        <rdfs:label>Observation</rdfs:label>
        <skos:prefLabel xml:lang="en">Observation</skos:prefLabel>
    </owl:Class>
    <owl:Restriction rdf:nodeID="genid80">
        <owl:onProperty rdf:resource="https://omac.crimproject.org/concerns"/>
        <owl:someValuesFrom>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/Person"/>
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/Place"/>
                    <rdf:Description rdf:about="https://omac.crimproject.org/Attribute"/>
                    <rdf:Description rdf:about="https://omac.crimproject.org/Expression"/>
                    <rdf:Description rdf:about="https://omac.crimproject.org/MusicalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </owl:someValuesFrom>
    </owl:Restriction>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://omac.crimproject.org/Observation"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
        <owl:annotatedTarget rdf:nodeID="genid80"/>
        <rdfs:comment>In principle observations can concern all domain entities</rdfs:comment>
    </owl:Axiom>
    


