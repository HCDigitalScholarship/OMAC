     -->

    


    <!-- http://dbpedia.org/ontology/Person -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/Person">
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/ontology/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Person</rdfs:label>
        <skos:prefLabel xml:lang="en">Person</skos:prefLabel>
    </owl:Class>
    


    <!-- http://dbpedia.org/ontology/Place -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/Place">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_part"/>
                <owl:allValuesFrom rdf:resource="http://dbpedia.org/ontology/Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#part_of"/>
                <owl:allValuesFrom rdf:resource="http://dbpedia.org/ontology/Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/ontology/</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place</rdfs:label>
        <skos:prefLabel xml:lang="en">Place</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#AnalyticSegment -->

    <owl:Class rdf:about="http://www.omac.org#AnalyticSegment">
        <rdfs:subClassOf rdf:resource="http://www.omac.org#Expression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#segment_of"/>
                <owl:someValuesFrom rdf:resource="http://www.omac.org#Score"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An analytic segment is a pattern of a musical work or musical work part identified by an analyst. It is not therefore an authorial part of a musical work</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Analytic Segment</rdfs:label>
        <skos:prefLabel xml:lang="en">Analytic Segment</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#Attribute -->

    <owl:Class rdf:about="http://www.omac.org#Attribute">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#attribute_of"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://dbpedia.org/ontology/Person"/>
                            <rdf:Description rdf:about="http://dbpedia.org/ontology/Place"/>
                            <rdf:Description rdf:about="http://www.omac.org#Expression"/>
                            <rdf:Description rdf:about="http://www.omac.org#MusicalEntity"/>
                            <rdf:Description rdf:about="http://www.omac.org#Observation"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_value"/>
                <owl:someValuesFrom rdf:resource="http://www.omac.org#AttributeValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The list of attributes is not exhaustive</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attribute</rdfs:label>
        <skos:prefLabel xml:lang="en">Attribute</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#AttributeValue -->

    <owl:Class rdf:about="http://www.omac.org#AttributeValue">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attribute Value</rdfs:label>
        <skos:prefLabel xml:lang="en">Attribute Value</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#Composer -->

    <owl:Class rdf:about="http://www.omac.org#Composer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://dbpedia.org/property/composer"/>
                        <owl:someValuesFrom rdf:resource="http://www.omac.org#MusicalEntity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A person who composes an authorial musical work</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Composer</rdfs:label>
        <skos:prefLabel xml:lang="en">Composer</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#Expression -->

    <owl:Class rdf:about="http://www.omac.org#Expression">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_part"/>
                <owl:allValuesFrom rdf:resource="http://www.omac.org#Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#part_of"/>
                <owl:allValuesFrom rdf:resource="http://www.omac.org#Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An expression is an abstract (and complex) pattern of characters in a certain notation (e.g., natural language, musical notes, etc)

It is not the specific material pattern of ink printed in a book</rdfs:comment>
        <rdfs:comment>To be possibly aligned with LRM</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Expression</rdfs:label>
        <skos:prefLabel xml:lang="en">Expression</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#GenreAttribute -->

    <owl:Class rdf:about="http://www.omac.org#GenreAttribute">
        <rdfs:subClassOf rdf:resource="http://www.omac.org#Attribute"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_value"/>
                <owl:someValuesFrom rdf:resource="http://www.omac.org#GenreValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_value"/>
                <owl:allValuesFrom rdf:resource="http://www.omac.org#GenreValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Genre</rdfs:label>
        <skos:altLabel xml:lang="en">Genre Attribute</skos:altLabel>
        <skos:prefLabel xml:lang="en">Genre</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#GenreValue -->

    <owl:Class rdf:about="http://www.omac.org#GenreValue">
        <rdfs:subClassOf rdf:resource="http://www.omac.org#AttributeValue"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Genre Value</rdfs:label>
        <skos:prefLabel xml:lang="en">Genre Value</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#KeyAttribute -->

    <owl:Class rdf:about="http://www.omac.org#KeyAttribute">
        <rdfs:subClassOf rdf:resource="http://www.omac.org#Attribute"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_value"/>
                <owl:someValuesFrom rdf:resource="http://www.omac.org#KeyValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_value"/>
                <owl:allValuesFrom rdf:resource="http://www.omac.org#KeyValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Key</rdfs:label>
        <skos:altLabel xml:lang="en">Key Attribute</skos:altLabel>
        <skos:prefLabel xml:lang="en">Key</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#KeyValue -->

    <owl:Class rdf:about="http://www.omac.org#KeyValue">
        <rdfs:subClassOf rdf:resource="http://www.omac.org#AttributeValue"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Key Value</rdfs:label>
        <skos:prefLabel xml:lang="en">Key Value</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#MechanicalTransformation -->

    <owl:Class rdf:about="http://www.omac.org#MechanicalTransformation">
        <rdfs:subClassOf rdf:resource="http://www.omac.org#Transformation"/>
        <skos:altLabel xml:lang="en">Mechanical Transformation Claim</skos:altLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#MusicalEntity -->

    <owl:Class rdf:about="http://www.omac.org#MusicalEntity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#composed_by"/>
                <owl:someValuesFrom rdf:resource="http://www.omac.org#Composer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_attribute"/>
                <owl:someValuesFrom rdf:resource="http://www.omac.org#Attribute"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_expression"/>
                <owl:someValuesFrom rdf:resource="http://www.omac.org#Score"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_part"/>
                <owl:allValuesFrom rdf:resource="http://www.omac.org#MusicalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#part_of"/>
                <owl:allValuesFrom rdf:resource="http://www.omac.org#MusicalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Musical Entity is a general modeling element to organize more specific classes in the taxonomy. 
It is not meant to be directly instantiated. It is not declared to be equivalent to its subclasses because we leave it open the possibility to further specialize its taxonomy.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Musical Entity</rdfs:label>
        <skos:prefLabel xml:lang="en">Musical Entity</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#MusicalSection -->

    <owl:Class rdf:about="http://www.omac.org#MusicalSection">
        <rdfs:subClassOf rdf:resource="http://www.omac.org#MusicalWorkPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#section_of"/>
                <owl:someValuesFrom rdf:resource="http://www.omac.org#MusicalWork"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#has_authorial_part"/>
                <owl:allValuesFrom rdf:resource="http://www.omac.org#MusicalSubsection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.omac.org#MusicalSubsection"/>
        <Example xml:lang="en">Each movement of Beethoven&apos;s Symphony No. 9</Example>
        <Example xml:lang="en">The Kyrie section of the Missa je suis dshrite by Jean Guyon</Example>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section</rdfs:label>
        <skos:altLabel xml:lang="en">movement</skos:altLabel>
        <skos:prefLabel xml:lang="en">Section</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#MusicalSubsection -->

    <owl:Class rdf:about="http://www.omac.org#MusicalSubsection">
        <rdfs:subClassOf rdf:resource="http://www.omac.org#MusicalWorkPart"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.omac.org#has_part"/>
                        <owl:someValuesFrom rdf:resource="http://www.omac.org#MusicalEntity"/>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#subsection_of"/>
                <owl:someValuesFrom rdf:resource="http://www.omac.org#MusicalSection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#subsection_of"/>
                <owl:someValuesFrom rdf:resource="http://www.omac.org#MusicalWork"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#subsection_of"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.omac.org#MusicalSection"/>
                            <rdf:Description rdf:about="http://www.omac.org#MusicalWork"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <Example xml:lang="en">The Christe as subsection of the Kyrie (section) of the Missa je suis dshrite by Jean Guyon</Example>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Included in OMAC work module</rdfs:comment>
        <rdfs:comment>Subsections are the smallest parts of musical works which can not be further decomposed in authorial musical entities</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subsection</rdfs:label>
        <skos:prefLabel xml:lang="en">Subsection</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.omac.org#MusicalWork -->

    <owl:Class rdf:about="http://www.omac.org#MusicalWork">
        <rdfs:subClassOf rdf:resource="http://www.omac.org#MusicalEntity"/>
        <owl:disjointWith rdf:resource="http://www.omac.org#MusicalWorkPart"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Experts tend to refer to a whole entity (i.e., an entity with possibly multiple authorial parts) with the expression &apos;musical work&apos;. This is the reason why we distinguish between Musical Work Part and Musical Work</rdfs:comment>
