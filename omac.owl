    <!-- http://www.omac.org#Observation -->

    <owl:Class rdf:about="http://www.omac.org#Observation">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.omac.org#concerns"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
