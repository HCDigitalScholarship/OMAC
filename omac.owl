<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.omac.org#"
     xml:base="http://www.omac.org"
     xmlns:dbo="http://dbpedia.org/ontology/"
     xmlns:dbp="http://dbpedia.org/property/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:obda="https://w3id.org/obda/vocabulary#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:viaf="http://viaf.org/viaf/"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:dcterms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://www.omac.org">
        <dcterms:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Emilio M. Sanfilippo, Laboratory for Applied Ontology, ISTC-CNR, Italy
Richard Freedman, Haverford College, USA</dcterms:creator>
        <dcterms:subject rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Music and Musicology</dcterms:subject>
        <owl:versionInfo>under development. This version covers both works and observations</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator">
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/dc/terms/</rdfs:isDefinedBy>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/terms/subject -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/subject">
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/dc/terms/</rdfs:isDefinedBy>
    </owl:AnnotationProperty>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.omac.org#Example -->

    <owl:AnnotationProperty rdf:about="http://www.omac.org#Example"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#prefLabel -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#prefLabel"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date">
        <rdfs:comment xml:lang="en">Common xsd:date format is: YYYY-MM-DD (e.g., 2001-10-26)</rdfs:comment>
    </rdfs:Datatype>
    


    <!-- http://www.w3.org/2001/XMLSchema#gYear -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dbpedia.org/property/birthPlace -->

    <owl:ObjectProperty rdf:about="http://dbpedia.org/property/birthPlace">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Person"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Place"/>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/property</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">birth place</rdfs:label>
        <skos:prefLabel xml:lang="en">birth place</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://dbpedia.org/property/composer -->

    <owl:ObjectProperty rdf:about="http://dbpedia.org/property/composer">
        <owl:inverseOf rdf:resource="http://www.omac.org#composed_by"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Person"/>
        <rdfs:range rdf:resource="http://www.omac.org#MusicalEntity"/>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://dbpedia.org/property</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">composer of</rdfs:label>
        <skos:definition xml:lang="en">The relation is used to bind a person to the musical entity that s/he has composed</skos:definition>
        <skos:prefLabel xml:lang="en">composer of</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://dbpedia.org/property/deathPlace -->

    <owl:ObjectProperty rdf:about="http://dbpedia.org/property/deathPlace">
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Person"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Place"/>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/property</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">death place</rdfs:label>
        <skos:prefLabel xml:lang="en">death place</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://dbpedia.org/property/genre -->

    <owl:ObjectProperty rdf:about="http://dbpedia.org/property/genre">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#has_attribute"/>
        <rdfs:domain rdf:resource="http://www.omac.org#MusicalEntity"/>
        <rdfs:range rdf:resource="http://www.omac.org#GenreAttribute"/>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/property</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has genre</rdfs:label>
        <skos:prefLabel xml:lang="en">has genre</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://dbpedia.org/property/key -->

    <owl:ObjectProperty rdf:about="http://dbpedia.org/property/key">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#has_attribute"/>
        <rdfs:domain rdf:resource="http://www.omac.org#MusicalEntity"/>
        <rdfs:range rdf:resource="http://www.omac.org#KeyAttribute"/>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/property</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has key</rdfs:label>
        <skos:altLabel xml:lang="en">key</skos:altLabel>
        <skos:prefLabel xml:lang="en">has key</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#attribute_of -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#attribute_of">
        <owl:inverseOf rdf:resource="http://www.omac.org#has_attribute"/>
        <rdfs:domain rdf:resource="http://www.omac.org#Attribute"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/Person"/>
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/Place"/>
                    <rdf:Description rdf:about="http://www.omac.org#Expression"/>
                    <rdf:Description rdf:about="http://www.omac.org#MusicalEntity"/>
                    <rdf:Description rdf:about="http://www.omac.org#Observation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>attribute of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#authorial_part_of -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#authorial_part_of">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#part_of"/>
        <owl:inverseOf rdf:resource="http://www.omac.org#has_authorial_part"/>
        <rdfs:domain rdf:resource="http://www.omac.org#MusicalWorkPart"/>
        <rdfs:range rdf:resource="http://www.omac.org#MusicalEntity"/>
        <Example rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The first movement of Beethoven&apos;s Symp. No. 9 is authorial part of the Symp. No. 9</Example>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">authorial part of</rdfs:label>
        <skos:definition xml:lang="en">The relation is used to represent the structure of musical entities, e.g., a symphony composed of movements and sections.</skos:definition>
        <skos:prefLabel xml:lang="en">authorial part of</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#composed_by -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#composed_by">
        <rdfs:domain rdf:resource="http://www.omac.org#MusicalEntity"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Person"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omac.org#authorial_part_of"/>
            <rdf:Description rdf:about="http://www.omac.org#composed_by"/>
        </owl:propertyChainAxiom>
        <Example rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Symphony No. 9 was composed by Ludwig van Beethoven</Example>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">composed by</rdfs:label>
        <skos:definition xml:lang="en">The relation is used to bind a musical entity to the person who composed it</skos:definition>
        <skos:prefLabel xml:lang="en">composed by</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#concerns -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#concerns">
        <owl:inverseOf rdf:resource="http://www.omac.org#related_to"/>
        <rdfs:domain rdf:resource="http://www.omac.org#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omac.org#concerns"/>
            <rdf:Description rdf:about="http://www.omac.org#expression_of"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omac.org#concerns"/>
            <rdf:Description rdf:about="http://www.omac.org#part_of"/>
        </owl:propertyChainAxiom>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">concerns</rdfs:label>
        <skos:prefLabel xml:lang="en">concerns</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#derivative_in -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#derivative_in">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#related_to"/>
        <owl:inverseOf rdf:resource="http://www.omac.org#has_derivative"/>
        <rdfs:domain rdf:resource="http://www.omac.org#Expression"/>
        <rdfs:range rdf:resource="http://www.omac.org#SimilarityObservation"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">derivative in</rdfs:label>
        <skos:prefLabel xml:lang="en">derivative in</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#derivative_of -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#derivative_of">
        <owl:inverseOf rdf:resource="http://www.omac.org#model_for"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omac.org#Expression"/>
                    <rdf:Description rdf:about="http://www.omac.org#MusicalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omac.org#Expression"/>
                    <rdf:Description rdf:about="http://www.omac.org#MusicalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">derivative of</rdfs:label>
        <skos:prefLabel xml:lang="en">derivative of</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#expression_of -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#expression_of">
        <owl:inverseOf rdf:resource="http://www.omac.org#has_expression"/>
        <rdfs:domain rdf:resource="http://www.omac.org#Expression"/>
        <rdfs:range rdf:resource="http://www.omac.org#MusicalEntity"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">expression of</rdfs:label>
        <skos:prefLabel xml:lang="en">expression of</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_analytic_segment -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_analytic_segment">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#has_part"/>
        <owl:inverseOf rdf:resource="http://www.omac.org#segment_of"/>
        <rdfs:domain rdf:resource="http://www.omac.org#Expression"/>
        <rdfs:range rdf:resource="http://www.omac.org#Expression"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has analytic segment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_attribute -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_attribute">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/Person"/>
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/Place"/>
                    <rdf:Description rdf:about="http://www.omac.org#Expression"/>
                    <rdf:Description rdf:about="http://www.omac.org#MusicalEntity"/>
                    <rdf:Description rdf:about="http://www.omac.org#Observation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.omac.org#Attribute"/>
        <rdfs:label>has attribute</rdfs:label>
        <skos:prefLabel xml:lang="en">has attribute</skos:prefLabel>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.omac.org#has_attribute"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#range"/>
        <owl:annotatedTarget rdf:resource="http://www.omac.org#Attribute"/>
        <rdfs:comment>The use of logical disjunction (OR) is used to allow a sort of modeling shortcut. For instance, saying that a work has-genre Credo, means that the work&apos;s genre-attribute has-value Credo.</rdfs:comment>
    </owl:Axiom>
    


    <!-- http://www.omac.org#has_authorial_part -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_authorial_part">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#has_part"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.omac.org#MusicalEntity"/>
        <rdfs:range rdf:resource="http://www.omac.org#MusicalWorkPart"/>
        <Example rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beethoven&apos;s Symp. No. 9 has authorial part the first, second, third, four movements</Example>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The relation is used to represent the structure of musical entities, e.g., a symphony composed of movements and sections.</rdfs:comment>
        <rdfs:label xml:lang="en">has authorial part</rdfs:label>
        <skos:prefLabel xml:lang="en">has authorial part</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_derivative -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_derivative">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#concerns"/>
        <rdfs:domain rdf:resource="http://www.omac.org#SimilarityObservation"/>
        <rdfs:range rdf:resource="http://www.omac.org#Expression"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has derivative</rdfs:label>
        <skos:prefLabel xml:lang="en">has derivative</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_expression -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_expression">
        <rdfs:domain rdf:resource="http://www.omac.org#MusicalEntity"/>
        <rdfs:range rdf:resource="http://www.omac.org#Expression"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has expression</rdfs:label>
        <skos:prefLabel xml:lang="en">has expression</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_model -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_model">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#concerns"/>
        <owl:inverseOf rdf:resource="http://www.omac.org#model_in"/>
        <rdfs:domain rdf:resource="http://www.omac.org#SimilarityObservation"/>
        <rdfs:range rdf:resource="http://www.omac.org#Expression"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has model</rdfs:label>
        <skos:prefLabel xml:lang="en">has model</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_part -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_part">
        <owl:inverseOf rdf:resource="http://www.omac.org#part_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">At the current state, parthood relations can hold across multiple branches of the ontology.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has part</rdfs:label>
        <skos:prefLabel xml:lang="en">has part</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_performing_force_type -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_performing_force_type">
        <rdfs:domain rdf:resource="http://www.omac.org#PerformingForceAttribute"/>
        <rdfs:range rdf:resource="http://www.omac.org#PerformingForceType"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has performing force type</rdfs:label>
        <skos:prefLabel xml:lang="en">has performing force type</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_schema -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_schema">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#has_attribute"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omac.org#Expression"/>
                    <rdf:Description rdf:about="http://www.omac.org#MusicalEntity"/>
                    <rdf:Description rdf:about="http://www.omac.org#Observation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.omac.org#SchemaAttribute"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has schema</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_section -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_section">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#has_authorial_part"/>
        <owl:inverseOf rdf:resource="http://www.omac.org#section_of"/>
        <rdfs:domain rdf:resource="http://www.omac.org#MusicalWork"/>
        <rdfs:range rdf:resource="http://www.omac.org#MusicalSection"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This relationship is used to model a musical work that is (authorial) section of another musical work. Sections could be further decomposed into sub-sections (see the relation: has subsection)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has section</rdfs:label>
        <skos:prefLabel xml:lang="en">has section</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_style -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_style">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#has_attribute"/>
        <rdfs:domain rdf:resource="http://www.omac.org#MusicalEntity"/>
        <rdfs:range rdf:resource="http://www.omac.org#StyleAttribute"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has style</rdfs:label>
        <skos:prefLabel xml:lang="en">has style</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_subsection -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_subsection">
        <rdfs:subPropertyOf rdf:resource="http://www.omac.org#has_authorial_part"/>
        <owl:inverseOf rdf:resource="http://www.omac.org#subsection_of"/>
        <rdfs:domain rdf:resource="http://www.omac.org#MusicalEntity"/>
        <rdfs:range rdf:resource="http://www.omac.org#MusicalSubsection"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omac.org#has_section"/>
            <rdf:Description rdf:about="http://www.omac.org#has_subsection"/>
        </owl:propertyChainAxiom>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Subsections are the smallest parts of musical works which can not be further decomposed in authorial musical entities</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has subsection</rdfs:label>
        <skos:prefLabel xml:lang="en">has subsection</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#has_value -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#has_value">
        <rdfs:domain rdf:resource="http://www.omac.org#Attribute"/>
        <rdfs:range rdf:resource="http://www.omac.org#AttributeValue"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has value</rdfs:label>
        <skos:prefLabel xml:lang="en">has value</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- http://www.omac.org#model_for -->

    <owl:ObjectProperty rdf:about="http://www.omac.org#model_for">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omac.org#Expression"/>
                    <rdf:Description rdf:about="http://www.omac.org#MusicalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.omac.org#Expression"/>
                    <rdf:Description rdf:about="http://www.omac.org#MusicalEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.omac.org#model_in"/>
            <rdf:Description rdf:about="http://www.omac.org#has_derivative"/>
        </owl:propertyChainAxiom>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relation used to represent an expression/musical entity that is used as model for another expression/musical entity (by the same or different composer)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">model for</rdfs:label>
    </owl:ObjectProperty>
    


